    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "serve": "node server.js",
    "prestart": "npm run build",
    "start": "npm run serve"
  },
